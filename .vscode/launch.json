{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "diff",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["cruncheevos", "diff", "${file}"],
      "envFile": "${fileDirname}/../.env",
      "env": {
        "FORCE_COLOR": "1"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "save",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["cruncheevos", "save", "${file}"],
      "envFile": "${fileDirname}/../.env",
      "env": {
        "FORCE_COLOR": "1"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "diff-save",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["cruncheevos", "diff-save", "${file}"],
      "envFile": "${fileDirname}/../.env",
      "env": {
        "FORCE_COLOR": "1"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "rich-save",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["cruncheevos", "rich-save", "--force-rewrite", "${file}"],
      "envFile": "${fileDirname}/../.env",
      "env": {
        "FORCE_COLOR": "1"
      }
    }
  ]
}
